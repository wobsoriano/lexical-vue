{
  "name": "lexical-vue",
  "type": "module",
  "version": "0.0.0",
  "description": "An extensible Vue 3 web text-editor based on Lexical.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/lexical-vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/lexical-vue.git"
  },
  "bugs": {
    "url": "https://github.com/wobsoriano/lexical-vue/issues"
  },
  "keywords": [
    "vue",
    "lexical",
    "editor",
    "rich-text"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./LexicalNodeMenuPlugin": {
      "types": "./dist/LexicalNodeMenuPlugin.vine.d.ts",
      "import": "./dist/LexicalNodeMenuPlugin.vine.js"
    },
    "./LexicalAutoEmbedPlugin": {
      "types": "./dist/LexicalAutoEmbedPlugin.vine.d.ts",
      "import": "./dist/LexicalAutoEmbedPlugin.vine.js"
    },
    "./LexicalAutoLinkPlugin": {
      "types": "./dist/LexicalAutoLinkPlugin.vine.d.ts",
      "import": "./dist/LexicalAutoLinkPlugin.vine.js"
    },
    "./LexicalTypeaheadMenuPlugin": {
      "types": "./dist/LexicalTypeaheadMenuPlugin.vine.d.ts",
      "import": "./dist/LexicalTypeaheadMenuPlugin.vine.js"
    },
    "./LexicalHorizontalRuleNode": {
      "types": "./dist/LexicalHorizontalRuleNode.d.ts",
      "import": "./dist/LexicalHorizontalRuleNode.js"
    },
    "./LexicalMarkdownShortcutPlugin": {
      "types": "./dist/LexicalMarkdownShortcutPlugin.vine.d.ts",
      "import": "./dist/LexicalMarkdownShortcutPlugin.vine.js"
    },
    "./LexicalAutoFocusPlugin": {
      "types": "./dist/LexicalAutoFocusPlugin.vine.d.ts",
      "import": "./dist/LexicalAutoFocusPlugin.vine.js"
    },
    "./LexicalAutoScrollPlugin": {
      "types": "./dist/LexicalAutoScrollPlugin.vine.d.ts",
      "import": "./dist/LexicalAutoScrollPlugin.vine.js"
    },
    "./LexicalBlockWithAlignableContents": {
      "types": "./dist/LexicalBlockWithAlignableContents.vine.d.ts",
      "import": "./dist/LexicalBlockWithAlignableContents.vine.js"
    },
    "./LexicalDecoratorBlockNode": {
      "types": "./dist/LexicalDecoratorBlockNode.d.ts",
      "import": "./dist/LexicalDecoratorBlockNode.js"
    },
    "./LexicalCharacterLimitPlugin": {
      "types": "./dist/LexicalCharacterLimitPlugin.vine.d.ts",
      "import": "./dist/LexicalCharacterLimitPlugin.vine.js"
    },
    "./LexicalCheckListPlugin": {
      "types": "./dist/LexicalCheckListPlugin.vine.d.ts",
      "import": "./dist/LexicalCheckListPlugin.vine.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build && vue-vine-tsc",
    "dev": "rslib build --watch"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@lexical/clipboard": "~0.37.0",
    "@lexical/code": "~0.37.0",
    "@lexical/devtools-core": "~0.37.0",
    "@lexical/dragon": "~0.37.0",
    "@lexical/extension": "~0.37.0",
    "@lexical/hashtag": "~0.37.0",
    "@lexical/history": "~0.37.0",
    "@lexical/html": "~0.37.0",
    "@lexical/link": "~0.37.0",
    "@lexical/list": "~0.37.0",
    "@lexical/mark": "~0.37.0",
    "@lexical/markdown": "~0.37.0",
    "@lexical/overflow": "~0.37.0",
    "@lexical/plain-text": "~0.37.0",
    "@lexical/rich-text": "~0.37.0",
    "@lexical/selection": "~0.37.0",
    "@lexical/table": "~0.37.0",
    "@lexical/text": "~0.37.0",
    "@lexical/utils": "~0.37.0",
    "@lexical/yjs": "~0.37.0",
    "lexical": "~0.37.0",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@rslib/core": "^0.15.1",
    "@vue-vine/rspack-loader": "1.7.14-beta.1",
    "vue": "^3.5.22",
    "vue-vine": "^1.7.14",
    "vue-vine-tsc": "^1.7.14",
    "yjs": "^13.6.18"
  }
}
